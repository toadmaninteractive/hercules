<UserControl x:Class="Hercules.Shell.View.BrowserPageView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:hc="clr-namespace:Hercules.Controls"
             hc:RoutedCommandBindings.CommandBindings="{Binding RoutedCommandBindings}"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <WebBrowser Focusable="True" hc:BrowserBehavior.BindableSource="{Binding Source}" />
</UserControl>
